{
  "version": "0.1.0",
  "name": "react-blogs",
  "type": "module",
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "react",
    "blog",
    "typescript",
    "article"
  ],
  "author": "Benjamin Norval",
  "homepage": "https://github.com/norvalbv/react-blogs",
  "main": "dist/index.umd.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "vite",
    "build": "vite build && yarn bundle:dts",
    "build:dev": "vite build --watch",
    "bundle:dts": "dts-bundle-generator -o ./dist/index.d.ts src/index.ts",
    "preview": "vite preview",
    "lint": "eslint src",
    "format:check": "prettier --check src",
    "format:fix": "prettier --write src",
    "test": "jest --watch --resetMocks --runInBand --onlyChanged"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.15.2",
    "js-yaml": "^4.1.0",
    "markdown-to-jsx": "^7.4.7",
    "prism-react-renderer": "^2.3.1",
    "zustand": "^4.5.2"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/react": "^18.3.3",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "@vanilla-extract/jest-transform": "^1.1.5",
    "@vanilla-extract/vite-plugin": "^4.0.10",
    "@vitejs/plugin-react": "^4.3.0",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.4.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-custom-rules": "^0.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.5",
    "pinst": "^3.0.0",
    "prettier": "^3.3.0",
    "react": "^18.3.1",
    "react-test-renderer": "^18.3.1",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vite-plugin-css-injected-by-js": "^3.5.1",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "packageManager": "yarn@4.2.2"
}
